@section scripts
{
    <script src="~/assets/global/plugins/jquery-repeater/jquery.repeater.js" type="text/javascript"></script>
    <script src="~/assets/global/plugins/bootstrap-datepicker/js/bootstrap-datepicker.min.js" type="text/javascript"></script>
    <script src="~/assets/admin/pages/scripts/form-repeater.min.js" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/knockout-3.4.0.js")"></script>

    <script>
        var sortSelect = function (select, attr, order) {
            if (attr === 'text') {
                if (order === 'asc') {
                    $(select).html($(select).children('option').sort(function (x, y) {
                        return $(x).text().toUpperCase() < $(y).text().toUpperCase() ? -1 : 1;
                    }));
                    $(select).get(0).selectedIndex = 0;
                }
            }

        };
        $(document).ready(function () {
            sortSelect('#sort', 'text', 'asc');
            $('#tblKerajaanBadanBerkanun').hide();
            $('#tblBerhad').hide();
            $('#tblEnterprise').hide();
            document.getElementById("tblSendirianBerhad").style.display = 'none';
        });

        $(function () {
            $('#companyTypeDropdown').change(function () {
                var value = $(this).val();
                if (value == "Berhad") {
                    $('#tblBerhad').show();
                    document.getElementById("tblSendirianBerhad").style.display = '';
                } else if (value != "Berhad" && value == "SendirianBerhad") {
                    $('#tblBerhad').show();
                    document.getElementById("tblSendirianBerhad").style.display = 'none';
                } else {
                    $('#tblBerhad').hide();
                    document.getElementById("tblSendirianBerhad").style.display = 'none';
                }
                if (value == "KerajaanBadanBerkanun") {
                    $('#tblKerajaanBadanBerkanun').show();
                } else {
                    $('#tblKerajaanBadanBerkanun').hide();
                }
                if (value == "EnterprisePerseorangan" || value == "EnterprisePerkongsian") {
                    $('#tblEnterprise').show();
                } else {
                    $('#tblEnterprise').hide();
                }
            });
        });
    </script>
}
