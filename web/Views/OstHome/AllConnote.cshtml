    <script type="text/javascript">
        @{ count = 1; }
        @foreach (var item in @Model.Consignments)
        {
            if (item.Produk.IsInternational)
            {
                <text>
                JsBarcode('#barcode-' + '@Html.Raw(count)', '@Html.Raw(item.ConNote)', {
                    width: 2,
                    height: 39
                });
                </text>
            }
            else
            {
                <text>
                $('#datamatrix-ndd-s1-' + '@Html.Raw(count)').barcode('@Html.Raw(item.ConNote)', 'datamatrix');
                $('#datamatrix-ndd-s2-' + '@Html.Raw(count)').barcode('@Html.Raw(item.ConNote)', 'datamatrix');
                JsBarcode('#barcode-' + '@Html.Raw(count)', '@Html.Raw(item.ConNote)', {
                    textPosition: "top",
                    textMargin: 5,
                    width: 2,
                    height: 50
                });
                </text>
            }
            count++;
        }
        window.print();
    </script>
