    <script>
        function textTo128(str, id) {
            /*
             * Generate 128 Barcode text, suitable for copying and pasting.
             */
            var len = str.length; //str.length - get length of string, used to generate the checksum.
            var type128 = 104; // 128 Type B start
            var typeClose = 106;
            var total = 104;
            var i; // Counter Variable
            for (i = 0; i < len; i++) {
                total += ((i + 1) * (str.charCodeAt(i) - 32)); //Multiply char position with decimal value of character, keep running total
            }
            var modVal = total % 103; // Use Modulus to find our checksum
            var checksum = String.fromCharCode(modVal + 32);

            if (modVal + 32 > 126) { alert(modVal + 32); };
            $('#barcode-ndd-s1-' + id).text(String.fromCharCode(type128 + 100) + str + checksum + String.fromCharCode(typeClose + 100));
            $('#barcode-ndd-s2-' + id).text(String.fromCharCode(type128 + 100) + str + checksum + String.fromCharCode(typeClose + 100));
            $('#barcode-ems-s1-' + id).text(String.fromCharCode(type128 + 100) + str + checksum + String.fromCharCode(typeClose + 100));
            $('#barcode-ems-s2-' + id).text(String.fromCharCode(type128 + 100) + str + checksum + String.fromCharCode(typeClose + 100));
        }

        jQuery(document).ready(function () {
            window.print();

        });
    </script>
    <script type="text/javascript">
        @{ count = 1; }
        @foreach (var item in @Model.Consignments)
        {
            <text>
        textTo128('@Html.Raw(item.ConNote)', '@Html.Raw(count)');
        $('#datamatrix-ndd-s1-' + '@Html.Raw(count)').barcode('@Html.Raw(item.ConNote)', 'datamatrix');
        $('#datamatrix-ndd-s2-' + '@Html.Raw(count)').barcode('@Html.Raw(item.ConNote)', 'datamatrix');
        </text>
            count++;
        }
    </script>
