@model Bespoke.Ost.ConsigmentRequests.Domain.Consignment
@{
    Layout = null;
}
<!DOCTYPE html>
<!--[if IE 8]> <html lang="en" class="ie8 no-js"> <![endif]-->
<!--[if IE 9]> <html lang="en" class="ie9 no-js"> <![endif]-->
<!--[if !IE]><!-->
<html lang="en">
<!--<![endif]-->
<!-- BEGIN HEAD -->
<head>

    <meta charset="utf-8" />
    <title>Pos Laju EziSend</title>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta content="width=device-width, initial-scale=1" name="viewport" />
    <meta name="description" content="Online Shipping Tool, Pos Malaysia Berhad" />
    <meta content="" name="author" />
    <!-- BEGIN GLOBAL MANDATORY STYLES -->
    <link href="http://fonts.googleapis.com/css?family=Open+Sans:400,300,600,700&subset=all" rel="stylesheet" type="text/css" />
    <link href="~/assets/global/plugins/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css" />
    <link href="~/assets/global/plugins/simple-line-icons/simple-line-icons.min.css" rel="stylesheet" type="text/css" />
    <link href="~/assets/global/plugins/bootstrap/css/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="~/assets/global/plugins/bootstrap-switch/css/bootstrap-switch.min.css" rel="stylesheet" type="text/css" />
    <!-- END GLOBAL MANDATORY STYLES -->
    <!-- BEGIN THEME GLOBAL STYLES -->
    <link href="~/assets/global/css/components.min.css" rel="stylesheet" id="style_components" type="text/css" />
    <link href="~/assets/global/css/plugins.min.css" rel="stylesheet" type="text/css" />
    <!-- END THEME GLOBAL STYLES -->
    <!-- BEGIN PAGE LEVEL STYLES -->
    <link href="~/assets/admin/pages/css/invoice.min.css" rel="stylesheet" type="text/css" />
    <!-- END PAGE LEVEL STYLES -->
    <!-- BEGIN THEME LAYOUT STYLES -->
    <link href="~/assets/admin/layouts/layout/css/layout.min.css" rel="stylesheet" type="text/css" />
    @*<link href="~/assets/admin/layouts/layout/css/themes/darkblue.min.css" rel="stylesheet" type="text/css" id="style_color" />*@
    <link href="~/assets/admin/layouts/layout/css/custom.min.css" rel="stylesheet" type="text/css" />
    <!-- END THEME LAYOUT STYLES -->
    <link rel="shortcut icon" href="favicon.ico" />
</head>
<!-- END HEAD -->
<body>
    <!-- BEGIN CONTENT BODY -->
    <div class="page-content">
        <div class="row">
            <br /><br />
            <div class="col-md-2"></div>
            <div class="col-md-8">
                <div class="invoice">
                    <div class="row">
                        <div class="col-md-2"></div>
                        <div class="col-md-8">
                            <center>
                                <p>
                                    <h3 class="bold">COMMERCIAL INVOICE</h3>
                                </p>
                            </center>
                        </div>
                        <div class="col-md-2">
                            <div class="pull-right">
                                <a class="btn btn-circle btn-sm yellow-casablanca hidden-print margin-bottom-10" onclick="javascript:window.print();">
                                    Print
                                    <i class="fa fa-print"></i>
                                </a>
                            </div>
                        </div>
                    </div>
                    <br /><br /><br />
                    <div class="row">
                        <div class="col-xs-4">
                            <h3>Sender:</h3>
                            <ul class="list-unstyled">
                                <li> <strong><span class="text-capitalize">@Html.Raw(Model.Pemberi.ContactPerson)</span></strong> </li>
                                <li> <span>@Html.Raw(Model.Pemberi.Address.Address1)</span>, <span>@Html.Raw(Model.Pemberi.Address.Address2)</span> </li>
                                <li> <span>@Html.Raw(Model.Pemberi.Address.Address3)</span>, <span>@Html.Raw(Model.Pemberi.Address.Address4)</span> </li>
                                <li> <span>@Html.Raw(Model.Pemberi.Address.Postcode)</span> <span>@Html.Raw(Model.Pemberi.Address.City)</span> </li>
                                <li> <span>@Html.Raw(Model.Pemberi.Address.State)</span> </li>
                                @*<li> <span>@Html.Raw(Model.Pemberi.Address.Country)</span> </li>*@
                                <li> <span>Malaysia</span> </li>
                            </ul>
                        </div>
                        <div class="col-xs-4">
                        </div>
                        <div class="col-xs-4 invoice-payment">
                            <h3>&nbsp;</h3>
                            <ul class="list-unstyled">
                                <li>
                                    <strong>Date:</strong> <span>@DateTime.Today.Date.ToShortDateString()</span>
                                </li>
                                <li>
                                    <strong>Airwaybill No:</strong> <span>@Html.Raw(Model.ConNote)</span>
                                </li>
                                <li>
                                    <strong>Phone No:</strong> +60<span>@Html.Raw(Model.Penerima.ContactInformation.ContactNumber)</span>
                                </li>
                                <li>
                                    <strong>Fax No:</strong> +60<span>@Html.Raw(Model.Penerima.ContactInformation.AlternativeContactNumber)</span>
                                </li>
                            </ul>
                        </div>
                    </div>
                    <hr />
                    <div class="row">
                        <div class="col-xs-4">
                            <h3>Recepient:</h3>
                            <ul class="list-unstyled">
                                <li> <strong><span class="text-capitalize">@Html.Raw(Model.Penerima.ContactPerson)</span></strong> </li>
                                <li> <span>@Html.Raw(Model.Penerima.Address.Address1)</span>, <span>@Html.Raw(Model.Penerima.Address.Address2)</span> </li>
                                <li> <span>@Html.Raw(Model.Penerima.Address.Address3)</span>, <span>@Html.Raw(Model.Penerima.Address.Address4)</span> </li>
                                <li> <span>@Html.Raw(Model.Penerima.Address.Postcode)</span> <span>@Html.Raw(Model.Penerima.Address.City)</span> </li>
                                <li> <span>@Html.Raw(Model.Penerima.Address.State)</span> </li>
                                <li> <span>@Html.Raw(Model.Penerima.Address.Country)</span> </li>
                                @*<li> <span>Malaysia</span> </li>*@
                            </ul>
                        </div>
                        <div class="col-xs-4">
                        </div>
                        <div class="col-xs-4 invoice-payment">
                            <h3>&nbsp;</h3>
                            <ul class="list-unstyled">
                                <li>
                                    <strong>Phone No:</strong> +60<span>@Html.Raw(Model.Penerima.ContactInformation.ContactNumber)</span>
                                </li>
                                <li>
                                    <strong>Fax No:</strong> +60<span>@Html.Raw(Model.Penerima.ContactInformation.AlternativeContactNumber)</span>
                                </li>
                            </ul>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-xs-12">
                            <table class="table table-striped table-hover">
                                <thead>
                                    <tr>
                                        <th> # </th>
                                        <th> Item </th>
                                        <th class="hidden-xs"> Box No </th>
                                        <th class="hidden-xs"> Origin Country </th>
                                        <th class="hidden-xs"> Quantity (pcs) </th>
                                        <th class="hidden-xs"> Value (pcs) </th>
                                        <th> Total </th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @*@foreach (var item in Model.Produk.Description)
                                        {

                                        }*@
                                    <tr>
                                        <td> 1 </td>
                                        <td> <span>@Html.Raw(Model.Produk.Description)</span> </td>
                                        <td class="hidden-xs"> 1 of 1 </td>
                                        <td class="hidden-xs"> <span>@Html.Raw(Model.Penerima.Address.Country)</span> </td>
                                        <td> 1 </td>
                                        <td> MYR <span>@Html.Raw(Model.Produk.ValueAddedDeclaredValue)</span> </td>
                                        <td> MYR <span>@Html.Raw(Model.Produk.Price)</span> </td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                    </div>
                    <br /><br />
                    <div class="row">
                        <div class="col-xs-4">
                            <div>
                                <address>
                                    <strong>Total Weight:</strong> <span>@Html.Raw(Model.Produk.Weight)</span> kg
                                </address>
                                <address>
                                    <strong>Dim Weight:</strong> <span>@Html.Raw(Model.Bill.VolumetricWeight)</span> kg
                                </address>
                            </div>
                        </div>
                        <div class="col-xs-8 invoice-block">
                            <ul class="list-unstyled amounts">
                                <li>
                                    <strong>Total Invoice Value:</strong> <span>@Html.Raw(Model.Produk.Price)</span>
                                </li>
                            </ul>
                            <br />
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-md-2"></div>
        </div>
    </div>
    <!-- END CONTENT BODY -->
    <!--[if lt IE 9]>
    <script src="~/assets/global/plugins/respond.min.js"></script>
    <script src="~/assets/global/plugins/excanvas.min.js"></script>
