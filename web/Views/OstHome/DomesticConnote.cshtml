    <script>
        function textTo128(str) {
            /*
             * Generate 128 Barcode text, suitable for copying and pasting.
             */
            var len = str.length; //str.length - get length of string, used to generate the checksum.
            var type128 = 104; // 128 Type B start
            var typeClose = 106;
            var total = 104;
            var i; // Counter Variable
            for (i = 0; i < len; i++) {
                total += ((i + 1) * (str.charCodeAt(i) - 32)); //Multiply char position with decimal value of character, keep running total
            }
            var modVal = total % 103; // Use Modulus to find our checksum
            var checksum = String.fromCharCode(modVal + 32);

            if (modVal + 32 > 126) { alert(modVal + 32); };
            $("#my-connote1").text(String.fromCharCode(type128 + 100) + str + checksum + String.fromCharCode(typeClose + 100));
            $("#my-connote2").text(String.fromCharCode(type128 + 100) + str + checksum + String.fromCharCode(typeClose + 100));
        }
        textTo128('@Html.Raw(Model.consignment.ConNote)');
        jQuery(document).ready(function () {
            window.print();

        });
    </script>
    <script>
        $(document).ready(function () {
            var text = ('@Html.Raw(Model.consignment.ConNote)');
            $("#bcTarget1").barcode(text, "datamatrix");
            $("#bcTarget2").barcode(text, "datamatrix");
        });
    </script>
