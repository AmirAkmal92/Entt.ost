            <script>
                function SignUp(){
                    ValidateResult = ValidateSignUp();
                    var email = $(".quick-reg-form [name=txt_email]").val();
                    email = email.trim();
                    if(ValidateResult==true){
                        return true;
                    }else{
                        window.exist = 0;
                        swal({
                            title: 'Oops',
                            type: 'error',
                            html: '' + ValidateResult,
                            confirmButtonColor: '#4e97d8'
                        })
                        return false;
                    }
                }

                function checkEmail(obj,check){
                    var pass = true;
                    var email = $(".quick-reg-form [name=txt_email]").val();
                    email = email.trim();

                    if(General.isTextEmpty($(obj)) || !General.validateEmail(email)){
                        doFail(obj);
                        window.outcome = 0;
                    }else{
                        doPadding(obj);
                        if(check){
                            $.ajax({
                                type: "POST",
                                url: "http://easyparcel.my/?ac=CheckUserEmail",
                                data : {
                                    email : email
                                },
                                async : false,
                                success:function(response){
                                    if(response == "pass"){
                                        doPass(obj);
                                        window.outcome = 2;
                                    }else{
                                        doFail(obj);
                                        window.outcome = 1;
                                    }
                                }
                            });
                        }
                    }
                }

                function checkEmpty(obj){
                    if(!General.isTextEmpty($(obj))){
                        doPass(obj);
                    }else{
                        doFail(obj);
                    }
                }

                function hasValidate(obj){
                    if($(obj).hasClass("pass") || $(obj).hasClass("padding") || $(obj).hasClass("fail"))
                        return true;
                    else
                        return false;
                }

                function doPass(obj){
                    $(obj).addClass("pass");
                    $(obj).removeClass("padding");
                    $(obj).removeClass("fail");
                    $(obj).css("border-color","#0F0");
                }

                function doPadding(obj){
                    $(obj).addClass("padding");
                    $(obj).removeClass("pass");
                    $(obj).removeClass("fail");
                    $(obj).css("border-color","#F90");
                }

                function doFail(obj){
                    $(obj).addClass("fail");
                    $(obj).removeClass("padding");
                    $(obj).removeClass("pass");
                    $(obj).css("border-color","#F00");
                }

                function ValidateSignUp(){
                    var email = $(".quick-reg-form [name=txt_email]");
                    var returnResult = "";
                    checkEmail(email,true);
                    if(window.outcome == 0){
                        returnResult += "Please Insert A Valid Email Into The Email Field.";
                    }else if(window.outcome == 1){
                        returnResult += "Another User With This Email Already Exists. Maybe It's Your Evil Twin. Spooky. If It Is You, Kindly Log In With The Email Or Click <br><a href='http://secure.easyparcel.my/pass/?pg=MemberForgotPassword'><b><u>HERE</u></b></a> To Reset A New Password.";;
                    }else if(window.outcome == 2){
                        returnResult = true;
                    }

                    return returnResult;
                }
            </script>
            <script>
                /*
