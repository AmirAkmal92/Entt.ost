                                        $.ajax({
                                            type  : "post",
                                            url : "http://secure.easyparcel.my/pass/?ac=checkPostcode",
                                            async : false,
                                            data : {
                                                cpostcode : $('#icp').val(),
                                                cstate : c_state,
