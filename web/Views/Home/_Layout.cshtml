<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <title>Pos Malaysia | @ViewBag.Title</title>
    <meta name="description" content="Online Shipping Tool, Pos Malaysia Berhad" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta property="og:image" content="/assets/custom/images/easyparcel-my-referralbanner.jpg" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <link href="@Url.Content("/Content/bootstrap.min.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("/Content/font-awesome.min.css")" rel="stylesheet" type="text/css" />
    <!-- Favicons-->
    <link rel="icon" type="image/png" href="assets/custom/favicon.ico" />
    <!-- BEGIN GLOBAL MANDATORY STYLES -->
    <link href="http://fonts.googleapis.com/css?family=Open+Sans:400,300,600,700&subset=all" rel="stylesheet" type="text/css" />
    <link href="/assets/global/plugins/simple-line-icons/simple-line-icons.min.css" rel="stylesheet" type="text/css" />
    <link href="/assets/global/plugins/bootstrap/css/bootstrap.css" rel="stylesheet" type="text/css" />
    <!-- END GLOBAL MANDATORY STYLES -->
    <!-- STYLES -->
    <link href="/assets/global/css/components-md.css" rel="stylesheet" type="text/css" />
    <link href="/assets/global/css/plugins-md.min.css" rel="stylesheet" type="text/css" />
    <!-- END STYLES -->
    <!-- BOOTSTRAP -->
    <link type="text/css" rel="stylesheet" href="/assets/custom/css/bootstrap-select.min.css" />
    <!-- END BOOTSTRAP -->
    <!-- Stylesheet-->
    <link type="text/css" rel="stylesheet" href="/assets/custom/css/fontello.css" />
    <link type="text/css" rel="stylesheet" href="/assets/custom/css/style.css" />
    <link type="text/css" rel="stylesheet" href="/assets/custom/css/jquery-ui.min.css" />
    <link type="text/css" rel="stylesheet" href="/assets/custom/css/animation.css" />
    <link type="text/css" rel="stylesheet" href="/assets/custom/css/nunito.css" />
    <link type="text/css" rel="stylesheet" href="/assets/custom/css/animate.css" />
    <link type="text/css" rel="stylesheet" href="/assets/custom/css/sweetalert2.css" />
    <link type="text/css" rel="stylesheet" href="/assets/custom/css/sweetalert2.min.css" />

    <!-- SCRIPT -->
    <script src="@Url.Content("/Scripts/jquery-2.2.0.min.js")"></script>
    <script src="@Url.Content("/Scripts/bootstrap.min.js")"></script>
    <script src="~/Scripts/knockout-3.4.0.js"></script>

    <!-- BEGIN CORE PLUGINS -->
    <script src="~/assets/global/plugins/jquery.min.js" type="text/javascript"></script>
    <script src="~/assets/global/plugins/js.cookie.min.js" type="text/javascript"></script>
    <script src="~/assets/global/plugins/jquery-slimscroll/jquery.slimscroll.min.js" type="text/javascript"></script>
    <script src="~/assets/global/plugins/jquery.blockui.min.js" type="text/javascript"></script>
    <script src="~/assets/global/plugins/bootstrap-switch/js/bootstrap-switch.min.js" type="text/javascript"></script>
    <!-- END CORE PLUGINS -->
    <script src="/Scripts/general.js" type="text/javascript"></script>
    <!-- End Javascript-->
    <!-- Original Javascript-->
    <script type="text/javascript" src="/assets/custom/js/jquery-migrate-1.2.1.min.js"></script>
    <script type="text/javascript" src="/assets/custom/js/bootstrap-filestyle.min.js"></script>
    <script type="text/javascript" src="/assets/custom/js/jquery.jplist.3.4.js"></script>
    <script type="text/javascript" src="/assets/custom/js/general.js"></script>
    <script type="text/javascript" src="/assets/custom/js/newsline.js"></script>
    <script type="text/javascript" src="/assets/custom/js/jssor.js"></script>
    <script type="text/javascript" src="/assets/custom/js/jssor.slider.js"></script>
    <script type="text/javascript" src="/assets/custom/js/jquery.viewportchecker.js"></script>
    <script type="text/javascript" src="/assets/custom/js/odometer.min.js"></script>
    <script type="text/javascript" src="/assets/custom/js/jquery-ui.min.js"></script>
    <script type="text/javascript" src="/assets/custom/js/jquery.mobile.custom.min.js"></script>
    <script type="text/javascript" src="/assets/custom/js/jquery.ui.widget.js"></script>
    <script type="text/javascript" src="/assets/custom/js/jquery.iframe-transport.js"></script>
    <script type="text/javascript" src="/assets/custom/js/jquery.fileupload.js"></script>
    <script type="text/javascript" src="/assets/custom/js/bootstrap-select.min.js"></script>
    <script type="text/javascript" src="/assets/custom/js/tooltip.js"></script>
    <script type="text/javascript" src="/assets/custom/js/conversion.js"></script>
    <script type="text/javascript" src="/assets/custom/js/sweetalert2.min.js"></script>
</head>
<body>
    <div id="masterContent" [:style:]="">
        <div class="span6">

            @RenderBody()

            <section class="visible-xs visible-sm" style="background-color: #1EE2FF; padding: 10px 0px;"></section>
            <section class="pink-bg signup-bar hidden-xs">
                <div class="container">
                    <div class="col-md-10 col-xs-10">
                        <h3>Sign up now for the new Online Shipping Tool! </h3>
                    </div>
                    <div class="col-md-2 col-xs-2">
                        <a href="#" class="btn btn-primary pull-right">Sign Up Now</a>
                    </div>
                </div>
            </section>
            <a href="#" style="color:#fff;">
            </a><section class="pink-bg signup-bar visible-xs" style="min-height: 60px;">
                <a href="#" style="color:#fff;">
                </a><div class="container">
                    <a href="#" style="color:#fff;">
                        <div class="col-md-10 col-xs-10">
                            <h3>Sign up now for the new Online Shipping Tool! </h3>
                        </div>
                    </a><div class="col-md-2 col-xs-2">
                        <a href="#" style="color:#fff;"><i class="epi-right-open" style="font-size: 22px; color: #fff; margin-top: 10px; position: absolute;"></i></a>
                    </div>
                </div>
            </section>
            <section class="steps-bg">
                <div class="container space-small">
                    <div class="col-sm-4 col-xs-12 steps padding-off">
                        <div class="col-md-4 col-sm-12 col-xs-4"><img src="assets/custom/images/compare.png" class="fade-in-down invisible" /></div>
                        <div class="col-md-8 col-sm-12 col-xs-8">
                            <h3><span style="color:#f69; font-size:36px;">1.</span> Compare Prices</h3>
                            <p>Use our price comparison engine to find the best prices today.</p>
                        </div>
                    </div>
                    <div class="col-xs-4 col-xs-offset-4 title-hr visible-xs">
                        <p class="hr-home"></p>
                    </div>
                    <div class="col-sm-4 col-xs-12 steps padding-off">
                        <div class="col-md-4 col-sm-12 col-xs-4"><img src="assets/custom/images/send.png" class="fade-in-down invisible" /></div>
                        <div class="col-md-8 col-sm-12 col-xs-8">
                            <h3><span style="color:#f69; font-size:36px;">2.</span> Send Parcels</h3>
                            <p>Send your parcel with our partners, we'll even come and collect it.</p>
                        </div>
                    </div>
                    <div class="col-xs-4 col-xs-offset-4 title-hr visible-xs">
                        <p class="hr-home"></p>
                    </div>
                    <div class="col-sm-4 col-xs-12 steps-last padding-off">
                        <div class="col-md-4 col-sm-12 col-xs-4"><img src="assets/custom/images/save.png" class="fade-in-down invisible" /></div>
                        <div class="col-md-8 col-sm-12 col-xs-8">
                            <h3><span style="color:#f69; font-size:36px;">3.</span> Save Money</h3>
                            <p>Our customers are saving money every time they use Pos OST!</p>
                        </div>
                    </div>
                </div>
            </section>
            <section class="light-bg stats-bg">
                <div class="container space">
                    <div class="container-fluid">
                        <div class="col-md-8 col-sm-12 join-happy-members center-space-md">
                            <h1>Online Shipping Tools by POS Malaysia!</h1>
                            <h3>Write Something.....</h3>
                        </div>
                    </div>
                </div>
            </section>
            <section class="pink-bg">
                <div class="container space quick-reg">
                    <div class="col-xs-12 col-md-8 quick-reg-form">
                        <h1>Free Registration</h1>
                        <form action="#" method="post">
                            <div class="col-md-6 col-sm-4 col-xs-12 res-email padding-left-off">
                                <input class="form-control" type="text" placeholder="Email" name="email" onblur="checkEmail(this,true)" />
                            </div>

                            <div class="col-md-6 col-sm-8 col-xs-12 padding-left-off">
                                <input class="btn btn-lg btn-primary" align="middle" type="Submit" value="Sign Up Now" />
                            </div>
                        </form>
                        <p>Sign up Now!</p>
                    </div>
                </div>
            </section>
            <div id="dialog"></div>
        </div>
        <div id="mainTrackResult"></div>
        <footer id="masterFooter" class="page-footer">
            <div class="container">
                <div class="footer-inner">
                    <div class="col-xs-6 col-sm-4 col-md-4 col-lg-2 footer-link-height">
                        <ul class="links">
                            <li class="title-link">Overview</li>
                            <li><a href="#">About Us</a></li>
                            <li><a href="#">FAQ</a></li>
                            <li><a href="#">Contact Us</a></li>
                            <li><a href="#">Prohibited Item List</a></li>
                            <li><a href="#">Terms &amp; Conditions</a></li>
                            <li><a href="#">Privacy Policy</a></li>
                        </ul>
                    </div>
                    <div class="col-xs-6 col-sm-4 col-md-4 col-lg-2 footer-link-height">
                        <ul class="links">
                            <li class="title-link">Quick Guide</li>
                            <li><a href="#">Sign Up An Account</a></li>
                            <li><a href="#">Place An Order</a></li>
                        </ul>
                    </div>
                    <div class="col-xs-6 col-sm-4 col-md-4 col-lg-2 footer-link-height">
                        <ul class="links">
                            <li class="title-link">My Account</li>
                            <li><a href="#">Log In</a></li>
                            <li><a href="#">Dashboard</a></li>
                            <li><a href="#">Edit Your Profile</a></li>
                            <li><a href="#">My Order</a></li>
                        </ul>
                    </div>
                    <div class="col-xs-6 col-sm-4 col-md-4 col-lg-2 footer-link-height">
                        <ul class="links">
                            <li class="title-link">Tools</li>
                            <li><a href="#">Send A Parcel</a></li>
                            <li><a href="#">Track &amp; Trace</a></li>
                            <li><a data-toggle="modal" href="#responsive">Volumetric Calculator</a></li>
                            <li><a href="#">Widgets</a></li>
                            <li><a href="#">Bulk Delivery Tool</a></li>
                        </ul>
                    </div>

                    <div class="col-xs-12 col-sm-6 col-md-4 footer-link-height social-links">
                        <ul class="links" style="padding-bottom: 5px;">
                            <li class="title-link">Follow Us</li>
                            <li style="float: left; margin-right: 10px;"><a href="#" target="_blank"><i class="epi-facebook"></i></a></li>
                            <li style="float: left; margin-right: 10px;"><a href="#" target="_blank"><i class="epi-twitter"></i></a></li>
                        </ul>
                        <hr style="margin: 15px 0px;border-color: #5d5d5d;" />
                    </div>
                </div>
            </div>
        </footer>
        <div class="page-copyright">
            <div class="container">
                <p class="text-center small">
                    © 2016 Pos Malaysia. All Rights Reserved
                </p>
            </div>
        </div>
        <div id="popupbox" class="modal fade in">
            <div class="modal-content">
                <div class="modal-header">
                    <h3>Loading...</h3>
                </div>
                <div class="modal-body"></div>
                <div class="modal-footer"></div>
            </div>
        </div>
        <!-- MODAL DIALOG -->
        <div class="swal2-container">
            <div class="swal2-overlay" tabindex="-1"></div>
            <div class="swal2-modal" style="display: none" tabindex="-1">
                <div class="swal2-icon swal2-error">
                    <span class="x-mark">
                        <span class="line left"></span>
                        <span class="line right"></span>
                    </span>
                </div>
                <div class="swal2-icon swal2-question">?</div>
                <div class="swal2-icon swal2-warning">!</div>
                <div class="swal2-icon swal2-info">i</div>
                <div class="swal2-icon swal2-success">
                    <span class="line tip"></span>
                    <span class="line long"></span>
                    <div class="placeholder"></div>
                    <div class="fix"></div>
                </div>
                <img class="swal2-image" />
                <h2></h2>
                <div class="swal2-content"></div>
                <input class="swal2-input" />
                <select class="swal2-select"></select>
                <div class="swal2-radio"></div>
                <label for="swal2-checkbox" class="swal2-checkbox">
                    <input type="checkbox" id="swal2-checkbox" />
                </label>
                <textarea class="swal2-textarea"></textarea>
                <div class="swal2-validationerror"></div>
                <hr class="swal2-spacer" />
                <button class="swal2-confirm">OK</button>
                <button class="swal2-cancel">Cancel</button>
                <span class="swal2-close">×</span>
            </div>
        </div>
        <!-- END MODAL DIALOG -->
    </div>
</body>
</html>